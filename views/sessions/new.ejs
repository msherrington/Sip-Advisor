<h1><img src="../assets/images/marker.png">Sip<span>Advisor</span></h1>
<h4>Login</h4>
<br>
<form method="POST" action="/login">
  <div>
    <label for="email">Email</label>
    <input type="text" name="email" id="email">
  </div>
  <div>
    <label for="password">Password</label>
    <input type="password" name="password" id="password">
  </div>

  <button class="button">Login</button>
</form>
<br>
<a href="<%= oauth.github.getLoginURL() %>"><img class="loginBtn" src="../assets/images/github.png"></a>
<a href="<%= oauth.facebook.getLoginURL() %>"><img class="loginBtn" src="../assets/images/facebook.png"></a>
